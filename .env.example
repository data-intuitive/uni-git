# Environment variables for integration tests
# Copy this file to .env and fill in your actual tokens

# GitLab Integration Tests
GITLAB_TOKEN=your_gitlab_personal_access_token_here
GITLAB_HOST=https://gitlab.com
# Optional: OAuth and Job tokens for additional auth method tests
GITLAB_OAUTH_TOKEN=your_gitlab_oauth_token_here
GITLAB_JOB_TOKEN=your_gitlab_job_token_here

# GitHub Integration Tests
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_HOST=https://api.github.com

# Bitbucket Integration Tests  
BITBUCKET_USERNAME=your_bitbucket_username_here
BITBUCKET_APP_PASSWORD=your_bitbucket_app_password_here
BITBUCKET_HOST=https://api.bitbucket.org
