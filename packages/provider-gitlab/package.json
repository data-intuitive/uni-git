{
  "name": "@uni-git/provider-gitlab",
  "version": "0.0.1",
  "description": "GitLab provider for unified Git API with support for GitLab.com and self-hosted GitLab instances. Supports personal access tokens, OAuth tokens, and job tokens.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --reporter=verbose --testNamePattern='Integration Tests' src/*.integration.test.ts"
  },
  "keywords": [
    "git",
    "gitlab",
    "api",
    "typescript",
    "provider",
    "self-hosted",
    "groups",
    "projects",
    "gitbeaker",
    "gitlab-api",
    "oauth",
    "personal-access-token",
    "job-token"
  ],
  "author": {
    "name": "Data Intuitive",
    "url": "https://data-intuitive.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/data-intuitive/uni-git#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/data-intuitive/uni-git.git",
    "directory": "packages/provider-gitlab"
  },
  "bugs": {
    "url": "https://github.com/data-intuitive/uni-git/issues"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@gitbeaker/rest": "^40"
  },
  "dependencies": {
    "@uni-git/core": "workspace:*"
  },
  "devDependencies": {
    "vitest": "^2"
  }
}
