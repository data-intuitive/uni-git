{
  "name": "@uni-git/core",
  "version": "0.0.1",
  "description": "Core interfaces and types for unified Git provider API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "git",
    "github",
    "gitlab",
    "bitbucket",
    "api",
    "typescript"
  ],
  "author": "Data Intuitive",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "vitest": "^2"
  }
}
